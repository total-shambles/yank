{
  "manifest_version": 3,
  "name": "SB_Wiz.pq & Ollama Extractor",
  "version": "1.0",
  "description": "Extracts 'sb_wiz.pq', sends a combined query to Ollama, and displays the response.",
  "permissions": ["tabs", "scripting", "notifications", "storage"],
  "host_permissions": ["<all_urls>", "http://localhost:11434/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "LS & Ollama Extractor"
  },
  "icons": {
    "16": "image.png",
    "32": "image.png",
    "64": "image.png",
    "128": "image.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}